
@{
    Layout = null;
}


<div class="col-sm-8 CreateProduct" style="display:none;;padding:15px;">
    <div class="form-group">
        <h3>Create Product</h3>
    </div>
    <div class="form-group">
        <input type="text" placeholder="Name Product" class="form-control" id="nameProduct" required="" />
    </div>
    <div class="form-group">
        <select class="form-control" id="idProduct"></select>
    </div>
    <div class="form-group">
        <input type="text" placeholder="Quantity" class="form-control" id="quantityProduct" required="" />
    </div>
    <div class="form-group">
        <input type="text" placeholder="Price" class="form-control" id="priceProduct" required="" />
    </div>
    <div class="form-group">
        <div id="exampleCreate" style="width:60%;"></div>
    </div>
    <div class="form-group">
        <input type="file" id="uploadFileProduct" class="form-control" />

    </div>
    <div class="form-group">
        <img src="" class="previewProduct" width="100" height="100" />
    </div>
    <button class="btn btn-primary backShowProduct">Back</button>&nbsp;&nbsp; <button class="btn btn-success" data-id="" id="CreateNewProduct">Create</button>
</div>
