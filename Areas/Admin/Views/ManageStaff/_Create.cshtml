
@{
    Layout = null;
}

<div class="col-sm-6 CreateStaff" style="display:none;padding: 15px;">
    <div class="form-group">
        <h3>Create Staff</h3>
    </div>
    <div class="form-group">
        <label for="nameStaff">Ten nhân viên</label>
        <input type="text" placeholder="Display Name" class="form-control" id="nameStaff" required="" />
    </div>

    <div class="form-group">
        <label for="phoneStaff">Số điện thoại</label>
        <input type="tel" placeholder="Phone" class="form-control" id="phoneStaff" required="" />
    </div>
    <div class="form-group">
        <label for="emailStaff">Email</label>
        <input type="email" placeholder="Email" class="form-control" id="emailStaff" required="" />
    </div>
    <div class="form-group">
        <label for="pwStaff">Password</label>
        <input type="password" placeholder="********" class="form-control" id="pwStaff" required="" />
    </div>
  
    <button class="btn btn-primary backShowStaff">Back</button>&nbsp;&nbsp; <button class="btn btn-success" data-id="" id="CreateNewStaff">Create</button>
</div>
